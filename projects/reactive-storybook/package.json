{
  "name": "@reformjs/reactive-storybook",
  "version": "0.0.2-rc.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "storybook2": "storybook dev",
    "start": "storybook",
    "lint": "npx eslint ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "watch": {
    "build:assets": {
      "patterns": [
        "src/assets"
      ],
      "extensions": "png,jpg,svg,gif",
      "ignore": "dist/",
      "quiet": true,
      "runOnChangeOnly": false
    },
    "build:styles": {
      "patterns": [
        "src/styles"
      ],
      "extensions": "scss,css",
      "quiet": true,
      "runOnChangeOnly": false
    },
    "build:js": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,json",
      "quiet": true,
      "runOnChangeOnly": false
    }
  },
  "eslintConfig": {
    "extends": "@reformjs/eslint-config"
  },
  "dependencies": {
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reformjs/reactive": "workspace:*",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "classnames": "^2.3.2",
    "date-fns": "2.30.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@reformjs/eslint-config": "workspace:*",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/webpack": "^6.2.1",
    "core-js": "3.4.1",
    "css-loader": "^6.7.2",
    "file-loader": "^6.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "lorem-ipsum": "^2.0.4",
    "mini-css-extract-plugin": "^2.1.0",
    "sass": "latest",
    "sass-package-importer": "latest",
    "pdfjs-dist": "2.6.347",
    "react-element-to-jsx-string": "^15.0.0",
    "resolve-url-loader": "3.1.4",
    "sass-loader": "^13.2.0",
    "storybook": "^7.0.6",
    "style-loader": "^3.3.1",
    "typescript": "~5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "5.7.3",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  }
}
