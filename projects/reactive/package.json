  "watch": {
    "watch:assets": {
      "patterns": [
        "src/assets"
      ],
      "extensions": "png,jpg,svg,gif",
      "ignore": "dist/",
      "quiet": false,
      "runOnChangeOnly": true
    },
    "watch:styles": {
      "patterns": [
        "src/styles"
      ],
      "extensions": "scss,css",
      "quiet": false,
      "runOnChangeOnly": true
    },
    "watch:js": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,json,ts,tsx",
      "quiet": false,
      "runOnChangeOnly": true
    }
